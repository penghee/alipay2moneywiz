{
  "name": "moneywiz-csv",
  "version": "1.0.0",
  "description": "generate moneywiz-formated .csv",
  "main": "index.js",
  "type": "module",
  "bin": "alipay.js",
  "scripts": {
    "alipay": "node alipay.js",
    "wechat": "node wechat.js",
    "icost": "node icost.js",
    "jd": "node jd.js",
    "import": "node import.js",
    "preview": "node preview.js",
    "build": "tsup",
    "pack:alipay": "cd dist && pkg alipay.cjs --targets node16-macos-x64 && cp alipay ../bin",
    "pack:wechat": "cd dist && pkg wechat.cjs --targets node16-macos-x64 && cp wechat ../bin"
  },
  "keywords": [
    "csv",
    "moneywiz"
  ],
  "author": "joe.w",
  "license": "ISC",
  "dependencies": {
    "csv": "^6.2.0",
    "iconv-lite": "^0.6.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "fs-extra": "^11.3.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
